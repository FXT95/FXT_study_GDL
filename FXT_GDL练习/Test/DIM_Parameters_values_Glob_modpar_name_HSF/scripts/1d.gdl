!-----------------------------------------------------VALUES "amount"range[1,)!------------------------------------------------------------!!!change the distance between two axis when "GLOB_MODPAR_NAME"change---------------------------------------------------------IF GLOB_MODPAR_NAME="distance" or  GLOB_MODPAR_NAME="amount"	THEN	if  VARDIM1(Arr_dis)<=amount THEN		FOR i=VARDIM1(Arr_dis)+1 TO amount		parameters Arr_dis[i]=dis		NEXT i	ELSE 		FOR i=amount+1 to VARDIM1(Arr_dis)		parameters Arr_dis[i]=0		NEXT i	ENDIF	FOR i=1 TO amount		IF Arr_dis[i]<=0.1 THEN			parameters Arr_dis[i]=dis		ENDIF	NEXT iENDIF!-------------------------------------------------------------------------------------------------------------------------------	!!!change the length of axis when "GLOB_MODPAR_NAME"change---------------------------------------------------------IF GLOB_MODPAR_NAME="distance" or  GLOB_MODPAR_NAME="amount"	THEN	if  VARDIM1(Arr_Axis_lenth)<=amount+1 THEN		FOR i=VARDIM1(Arr_Axis_lenth)+1 TO amount+1		parameters Arr_Axis_lenth[i]=Axis_lenth		NEXT i	ELSE 		FOR i=amount+2 to VARDIM1(Arr_Axis_lenth)		parameters Arr_Axis_lenth[i]=0		NEXT i	ENDIF	FOR i=1 TO amount+1		IF Arr_Axis_lenth[i]<=0.1 THEN			parameters Arr_Axis_lenth[i]=Axis_lenth		ENDIF	NEXT iENDIF!-------------------------------------------------------------------------------------------------------------------------------	