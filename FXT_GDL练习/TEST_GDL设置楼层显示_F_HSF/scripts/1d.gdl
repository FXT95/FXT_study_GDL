!=================================获取手动输入的 楼层===================DIM 	F_story[]i=1Index_SUB=STRSTR(F_Select_story,";")WHILE Index_SUB>0 DO	len=STRLEN(F_Select_story)	Index_SUB=STRSTR(F_Select_story,";")	F_story[i] =STRSUB(F_Select_story,1,Index_SUB-1)		F_Select_story=	STRSUB(F_Select_story,Index_SUB+1,len)	i=i+1ENDWHILEF_story[i-1]=F_Select_story				!先执行i=i+1 后防止元素  所以 i需要 减少一个DIM	Num_Story[]j=1FOR i =1 to VARDIM1(F_story)	IF STRSTR(F_story[i],"_")=0 THEN  		n=SPLIT(F_story[i],"%n",number0)		if Number0>0 THEN Number0=	Number0-1		Num_Story[j]=number0		j=j+1	ELSE		n=SPLIT(F_story[i],"%n_%n",number1,size1,number2)		if Number1>0 THEN Number1=	Number1-1		if Number2>0 THEN Number2=	Number2-1		FOR k=number1 to number2			Num_Story[j]=k			j=j+1		NEXT k		ENDIFNEXT i!print Num_Story!!!!!!!======================================================!!!!!!!!!=============调用系统楼层信息================================n=REQUEST("STORY","",F_Current_story,F_Current_story_name)n=REQUEST("HOME_STORY","",F_Home_story,F_Home_story_name)parameters	F_Home_story=F_Home_storyparameters	F_Current_story=F_Current_story!!!!!!!!!=============判断始位楼层是否再选择显示的范围内================================Kill_me=1for i =1 TO VARDIM1(Num_Story)	if F_Current_story=Num_Story[i] THEN Kill_me=0Next iEPS=0.0001IF   GLOB_MODPAR_NAME="BUTTON"	THEN		IF BUTTON	THEN					parameters	x=TEST_A			parameters	y=TEST_B		ElSE			parameters x=0			parameters y =0		ENDIF		ENDIF